        <a class="btn btn-success btn-md" href="/caissein" role="button"><i class="fa fa-plus"></i>   Nouveau Entree</a>
      </div>



    <div class="clearfix"></div>
  </div>

          <div class="x_content">


            <div class="table-responsive">
          <!--    <table class="table table-striped jambo_table bulk_action"> -->
              <table id="datatable-checkbox" class="table table-striped table-bordered bulk_action">
                <thead>
                  <tr class="headings">
                    <th>
                      <input type="checkbox" id="check-all" class="flat">
                    </th>
                    <th class="column-title">id </th>
                    <th class="column-title">date</th>
                    <th class="column-title">Motif </th>
                    <th class="column-title">Mode de Paiement</th>
                    <th class="column-title">Montant</th>
                    <th class="column-title no-link last"><span class="nobr">Action</span>
                    </th>
                    <th class="bulk-actions" colspan="7">
                      <a class="antoo" style="color:#fff; font-weight:500;">Bulk Actions ( <span class="action-cnt"> </span> ) <i class="fa fa-chevron-down"></i></a>
                    </th>
                  </tr>
                </thead>

                <tbody>
                  <% for(var i=0; i < caisse.length; i++) { %>
                  <tr class="even pointer">
                    <td class="a-center ">
                      <input type="checkbox" class="flat" name="table_records">
                    </td>
                    <td class=" "><%= caisse[i]._id %></td>
                    <td class=" "><%= caisse[i].datein %></td>
                    <td class=" "><%= caisse[i].motif %></td>
                    <td class=" "><%= caisse[i].modepaiement %></td>
                    <td class=" "><%= caisse[i].montant.toFixed(2) %></td>
                    <td class="last"  align='right' width='200px;'>
                      <a class="btn btn-info btn-xs" href="" role="button"><i class="fa fa-pencil"></i></a>
                      <a class="btn btn-primary btn-xs" href="" role="button"><i class="fa fa-eye"></i></a>
                      <!-- <a class="btn btn-danger btn-xs" href="/caisse/<%= caisse[i]._id %>" ><i class="fa fa-trash-o"></i></a> -->

                    </td>
                  </tr>
                  <% } %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
