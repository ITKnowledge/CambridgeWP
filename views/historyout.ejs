<% include header.ejs %>
<title> - Historique des sorties du stock <%= date %></title>
<!-- page content -->
<!-- page content -->
<div class="right_col" role="main">
  <div class="">
    <div class="row">

      <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
          <div class="x_title">
            <h2>Historique des sorties du stock <small>Gestion de Stock</small></h2>
              <div class="pull-right">
              </div>
              <div class="clearfix"></div>
          </div>

          <div class="x_content">
            <div class="table-responsive">
              <table id="datatable-buttons" class="table table-striped table-bordered bulk_action table-hover" border="1" cellpadding="1">
                <thead>
                  <tr class="headings">

                    <th class="column-title">Date sortie </th>
                    <th class="column-title">Dépôt </th>
                    <th class="column-title">Code produit </th>
                    <th class="column-title">Désignation produit </th>
                    <th class="column-title">Quantité </th>
                    <th class="column-title">date expiration </th>
                    <th class="column-title">Nature de sortie </th>


                  </tr>
                </thead>

                <tbody>
                  <% for(var i=0; i < stockout.length; i++) { %>
                    <% for(j=0; j < stockout[i].out.length; j++){%>
                      <tr class="even pointer">
                      <td class=" "><%= stockout[i].out[j].dateout.substring(0, 19).replace("T", " ") %></td>
                      <td class=" "><%= stockout[i].depotname %></td>
                      <td class=" "><%= stockout[i].prodcode %></td>
                      <td class=" "><%= stockout[i].prodname %></td>
                      <td class=" " style="color:Red" align="center"><%= stockout[i].out[j].qteout %></td>
                      <td class=" "><%= stockout[i].dateexp %></td>
                      <td class=" "><%= stockout[i].out[j].motifout %></td>

                    </tr>
                        <%}}%>
                  </tbody>
                </table>
          <!--    <table class="table table-striped jambo_table bulk_action"> -->

              </div>
              <!-- <button id="print" type="button" class="btn btn-primary">Imprimer</button> -->
              <button id="retour" type="button" class="btn btn-success" onclick="window.location.href='/listprod'">Liste des produits</button>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- /page content -->

  <!-- /page content -->

  <!-- myModal -->
  <div id="myModal" class="modal fade" role="dialog">
    <div class="modal-dialog">

      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">About</h4>
        </div>
        <div class="modal-body">
          <p id="test">test</p>
          <p>Thanks for you interest.</p>
          <quote>I hope this tool will help you.</quote>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        </div>
      </div>

    </div>
  </div>



  <script src="../build/js/Helper.js"></script>


  <% include footer.ejs %>


  <script>
  function printData()
  {
   var divToPrint=document.getElementById("datatable-checkbox");
   newWin= window.open("");
   newWin.document.write("<h3>Historique des sortie du stock</h3>");
   newWin.document.write(divToPrint.outerHTML);
   newWin.print();
   newWin.close();
  }

  $('#print').on('click',function(){
  printData();
  })
  </script>
